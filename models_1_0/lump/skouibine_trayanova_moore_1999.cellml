          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
                <minus/>
                <apply>
                  <divide/>
                  <apply>
                    <minus/>
                    <ci> V </ci>
                    <cn cellml:units="mV"> 5.0 </cn>
                  </apply>
                  <cn cellml:units="mV"> 13.89 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
