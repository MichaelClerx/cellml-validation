              <exp/>
              <apply>
                <minus/>
                <apply>
                  <divide/>
                  <apply>
                    <plus/>
                    <ci> V </ci>
                    <cn cellml:units="mV"> 44.0 </cn>
                  </apply>
                  <cn cellml:units="mV"> 59.0 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
                  <plus/>
                  <ci> V </ci>
                  <cn cellml:units="mV"> 44.0 </cn>
                </apply>
                <cn cellml:units="mV"> 20.0 </cn>
              </apply>
            </apply>
          </apply>
        </apply>
