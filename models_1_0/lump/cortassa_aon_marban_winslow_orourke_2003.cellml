          <apply>
            <times/>
            <ci> Kcat_CS </ci>
            <ci> ET_CS </ci>
          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <divide/>
