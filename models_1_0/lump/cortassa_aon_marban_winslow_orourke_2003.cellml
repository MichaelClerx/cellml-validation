                </apply>
                <apply>
                  <plus/>
                  <cn cellml:units="dimensionless"> 1.0 </cn>
                  <apply>
                    <divide/>
                    <ci> Ca_m </ci>
                    <ci> Kd_Ca </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
