              <ci> AcCoA </ci>
            </apply>
            <apply>
              <divide/>
              <ci> Km_OAA </ci>
              <ci> OAA </ci>
            </apply>
            <apply>
              <times/>
              <apply>
                <divide/>
                <ci> Km_AcCoA </ci>
                <ci> AcCoA </ci>
              </apply>
              <apply>
                <divide/>
                <ci> Km_OAA </ci>
                <ci> OAA </ci>
