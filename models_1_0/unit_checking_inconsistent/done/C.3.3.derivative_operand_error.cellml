<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, C.3.3: A derivative degree must be dimensionless -->
<model name="derivative_operand_error"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="time" units="dimensionless" initial_value="0" />
    <variable name="x" units="dimensionless" initial_value = "1" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <degree>
            <cn cellml:units="volt">2</cn>
          </degree>
          <ci>x</ci>
        </apply>
        <cn cellml:units="dimensionless">0.001</cn>
      </apply>
    </math>
  </component>
</model>
