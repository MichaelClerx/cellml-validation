<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, C.3.3: Operands of a minus must have the same unit. -->
<model name="unit_checking_arithmetic_minus_operand_error_1"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="volt" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>a</ci>
        <apply>
          <minus />
          <cn cellml:units="volt">3</cn>
          <cn cellml:units="kilogram">8.7</cn>
        </apply>
      </apply>
    </math>
  </component>
</model>
