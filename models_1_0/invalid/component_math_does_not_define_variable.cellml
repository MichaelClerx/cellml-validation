<?xml version="1.0" encoding="UTF-8"?>
<!-- Is this in the spec?: A component's math must define its variables -->
<model name="component_math_does_not_define_variable"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="c1">
    <variable name="a" cellml:units="dimensionless" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <cn cellml:units="dimensionless">1</cn>
        <cn cellml:units="dimensionless">1</cn>
      </apply>
    </math>
  </component>
</model>
