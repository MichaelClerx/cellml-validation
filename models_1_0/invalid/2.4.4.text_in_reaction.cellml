<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.4 A reaction cannot have text -->
<model name="text_in_reaction"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="a">
        <role role="reactant" />
      </variable_ref>
      Pears
    </reaction>
  </component>
</model>
