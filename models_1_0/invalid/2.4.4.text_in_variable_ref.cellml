<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.4 A variable_ref cannot have text -->
<model name="text_in_variable_ref"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="a">
        Banana
        <role role="reactant" />
      </variable_ref>
    </reaction>
  </component>
</model>
