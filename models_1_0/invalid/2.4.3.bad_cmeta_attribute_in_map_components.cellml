<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 2.4.3: cmeta attributes other than id may not appear in a
     map_components -->
<model name="bad_cmeta_attribute_in_map_components"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
  <connection>
    <map_components component_1="a" component_2="b" cmeta:bob="burger" />
    <map_variables variable_1="x" variable_2="x" />
  </connection>
  <component name="a">
    <variable name="x" units="newton" public_interface="out" />
  </component>
  <component name="b">
    <variable name="x" units="newton" public_interface="in" />
  </component>
</model>
