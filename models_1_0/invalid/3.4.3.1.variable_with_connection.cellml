<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 3.4.3.1: A variable cannot contain a connection -->
<model name="variable_with_connection"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="volt" public_interface="out" />
  </component>
  <component name="B">
    <variable name="a" units="volt" public_interface="in" />
  </component>
  <component name="C">
    <variable name="b" units="dimensionless">
      <connection>
        <map_components component_1="A" component_2="B" />
        <map_variables variable_1="a" variable_2="a" />
      </connection>
    </variable>
  </component>
</model>
