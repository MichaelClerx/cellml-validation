<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 5.4.1.1: A units cannot contain a reaction -->
<model name="units_with_reaction"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <units name="wooster">
      <unit units="volt" />
      <reaction>
        <variable_ref variable="x">
          <role role="reactant" />
        </variable_ref>
      </reaction>
    </units>
    <variable name="x" units="dimensionless" />
  </component>
</model>
