<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 7.4.2.1: A variable_ref cannot contain a model -->
<model name="variable_ref_with_model"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="a">
        <role role="reactant" />
        <model name="inner_model" />
      </variable_ref>
    </reaction>
  </component>
</model>
