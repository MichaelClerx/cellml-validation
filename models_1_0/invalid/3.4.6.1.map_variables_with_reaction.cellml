<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 3.4.6.1: A map_variables cannot contain a reaction -->
<model name="map_variables_with_reaction"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <connection>
    <map_components component_1="a" component_2="b" />
    <map_variables variable_1="x" variable_2="x">
      <reaction>
        <variable_ref variable="x">
          <role role="reactant" />
        </variable_ref>
      </reaction>
    </map_variables>
  </connection>
  <component name="a">
    <variable name="x" units="newton" public_interface="out" />
  </component>
  <component name="b">
    <variable name="x" units="newton" public_interface="in" />
  </component>
</model>
