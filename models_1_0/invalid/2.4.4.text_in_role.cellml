<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.4 A role cannot have text -->
<model name="text_in_role"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="a">
        <role role="reactant">Pineapple</role>
      </variable_ref>
    </reaction>
  </component>
</model>
