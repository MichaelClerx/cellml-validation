                          the child's public interface. -->
<!-- A.a straight to C.a  -->
<model name="map_variables_hidden_grandparent_2"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="newton" public_interface="out" initial_value="1" />
  </component>
  <component name="B" />
  <component name="C">
    <variable name="a" units="newton" public_interface="in" />
  </component>
  <group>
    <relationship_ref relationship="encapsulation" />
    <component_ref component="A" >
      <component_ref component="B" >
        <component_ref component="C" />
      </component_ref>
    </component_ref>
  </group>
  <connection>
    <map_components component_1="A" component_2="C" />
    <map_variables variable_1="a" variable_2="a" />
  </connection>
</model>
