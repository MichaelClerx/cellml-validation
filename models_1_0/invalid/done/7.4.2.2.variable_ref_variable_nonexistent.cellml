<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 7.4.2.2: A variable_ref must refer to a local variable -->
<model name="variable_ref_variable_nonexistent"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="b" />
    </reaction>
  </component>
</model>
