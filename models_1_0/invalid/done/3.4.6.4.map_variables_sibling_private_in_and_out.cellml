<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 3.4.6.4: Connections must have one "in", one "out" -->
<model name="map_variables_sibling_private_in_and_out"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="newton" private_interface="in" />
  </component>
  <component name="B">
    <variable name="a" units="newton" private_interface="out" initial_value="1" />
  </component>
  <connection>
    <map_components component_1="A" component_2="B" />
    <map_variables variable_1="a" variable_2="a" />
  </connection>
</model>
