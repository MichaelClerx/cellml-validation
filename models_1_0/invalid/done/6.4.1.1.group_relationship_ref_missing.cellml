<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 6.4.1.1, A group element must contain exactly one relationship_ref -->
<model name="group_relationship_ref_missing"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:fruit="http://fruit.org">
  <component name="A" />
  <component name="B" />
  <group>
    <component_ref component="A" >
      <component_ref component="B" />
    </component_ref>
    <fruit:apple />
  </group>
</model>
