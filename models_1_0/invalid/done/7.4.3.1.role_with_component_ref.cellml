<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 7.4.3.1: A role cannot contain a component_ref -->
<model name="role_with_component_ref"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="a">
        <role role="reactant">
          <component_ref component="A" />
        </role>
      </variable_ref>
    </reaction>
  </component>
</model>
