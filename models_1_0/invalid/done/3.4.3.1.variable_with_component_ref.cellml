<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 3.4.3.1: A variable cannot contain a component_ref -->
<model name="variable_with_component_ref"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="c1">
    <variable name="x" cellml:units="dimensionless">
      <component_ref component="c1" />
    </variable>
  </component>
</model>
