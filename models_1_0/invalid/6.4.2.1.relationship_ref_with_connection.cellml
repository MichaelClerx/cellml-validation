<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 6.4.2.1: A relationship_ref cannot contain a connection -->
<model name="relationship_ref_with_connection"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="volt" public_interface="out" />
  </component>
  <component name="B">
    <variable name="a" units="volt" public_interface="in" />
  </component>
  <group>
    <relationship_ref relationship="containment">
      <connection>
        <map_components component_1="A" component_2="B" />
        <map_variables variable_1="a" variable_2="a" />
      </connection>
    </relationship_ref>
    <component_ref component="A" >
      <component_ref component="B" />
    </component_ref>
  </group>
</model>
