<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 3.4.6.4: Siblings are connected via the public interface -->
<!-- B.a to C.a, both children of A -->
<model name="map_variables_nested_sibling_private_in"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A" />
  <component name="B">
    <variable name="a" units="newton" public_interface="out" initial_value="1" />
  </component>
  <component name="C">
    <variable name="a" units="newton" private_interface="in" />
  </component>
  <group>
    <relationship_ref relationship="encapsulation" />
    <component_ref component="A">
      <component_ref component="B" />
      <component_ref component="C" />
    </component_ref>
  </group>
  <connection>
    <map_components component_1="B" component_2="C" />
    <map_variables variable_1="a" variable_2="a" />
  </connection>
</model>
