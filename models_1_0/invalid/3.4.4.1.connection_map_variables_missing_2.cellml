<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 3.4.4.1: Connection must have 1 map_variables -->
<model name="connection_map_variables_missing_2"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:fruit="http://fruit.org">
  <component name="A">
    <variable name="a" units="meter" public_interface="out" />
  </component>
  <component name="B">
    <variable name="a" units="meter" public_interface="in" />
  </component>
  <connection>
    <map_components component_1="A" component_2="B" />
    <fruit:apple />
    <fruit:banana />
  </connection>
</model>
