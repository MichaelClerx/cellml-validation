<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 6.4.3.2: Children of a given component can only be declared in
                          one place -->
<model name="component_ref_children_declared_twice_1"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A" />
  <component name="B" />
  <component name="C" />
  <group>
    <relationship_ref relationship="containment" />
    <component_ref component="A" >
      <component_ref component="B" />
    </component_ref>
    <component_ref component="A" >
      <component_ref component="C" />
    </component_ref>
  </group>
</model>
