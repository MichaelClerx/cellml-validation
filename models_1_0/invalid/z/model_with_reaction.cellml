<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 3.4.1.1: A model cannot contain a reaction -->
<model name="model_with_reaction"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="c">
    <variable name="x" units="dimensionless" />
  </component>
  <reaction>
    <variable_ref variable="x">
      <role role="reactant" />
    </variable_ref>
  </reaction>
</model>
