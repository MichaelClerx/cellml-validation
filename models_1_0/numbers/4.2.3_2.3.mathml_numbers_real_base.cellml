<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 4.2.3: The MathML "CellML subset"
Numbers: Real numbers with a base attribute -->
<model name="mathml_numbers_real_base"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="real_base_10" units="volt" />
    <variable name="real_base_2" units="volt" />
    <variable name="real_base_16" units="volt" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>real_base_10</ci>
        <cn cellml:units="volt" type="real" base="10">1.9</cn>
      </apply>
      <apply>
        <eq/>
        <ci>real_base_2</ci>
        <cn cellml:units="volt" type="real" base="2">101.101</cn>
      </apply>
      <apply>
        <eq/>
        <ci>real_base_16</ci>
        <cn cellml:units="volt" type="real" base="2">1D.E</cn>
      </apply>
    </math>
  </component>
</model>
