<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 5.4.2.2: A unit can reference local units: and unit trees can
     be quite complicated and specified in any order -->
<model name="unit_units_local_1"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <units name="fluther">
    <unit units="second" />
  </units>
  <units name="milliwooster_per_fluther">
    <unit units="wooster" prefix="milli" />
    <unit units="fluther" exponent="-1" />
  </units>
  <units name="wooster">
    <unit units="volt" />
  </units>
  <units name="megawoosterfluther3">
    <unit units="wooster" prefix="mega" />
    <unit units="fluther" exponent="3" />
  </units>
</model>
