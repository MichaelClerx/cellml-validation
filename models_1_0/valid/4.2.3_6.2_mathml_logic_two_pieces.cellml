<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 4.2.3: The MathML "CellML subset"
Piecewise value: piecewise, piece, otherwise and eq -->
<model name="mathml_logic_two_pieces"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="x" units="dimensionless" initial_value="0" />
    <variable name="y" units="dimensionless" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>y</ci>
        <piecewise>
          <piece>
            <cn cellml:units="dimensionless">123</cn>
            <apply>
              <eq />
              <ci>x</ci>
              <cn cellml:units="dimensionless">0</cn>
            </apply>
          </piece>
          <piece>
            <cn cellml:units="dimensionless">456</cn>
            <apply>
              <eq />
              <ci>x</ci>
              <cn cellml:units="dimensionless">1</cn>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="dimensionless">0</cn>
          </otherwise>
        </piecewise>
      </apply>
    </math>
  </component>
</model>
