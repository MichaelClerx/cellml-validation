<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 4.2.3: The MathML "CellML subset"
Binary logical operators: and, or, xor -->
<model name="mathml_logic_binary_operators"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="x" units="dimensionless" initial_value="0" />
    <variable name="y" units="dimensionless" initial_value="1" />
    <variable name="and" units="dimensionless" />
    <variable name="or" units="dimensionless" />
    <variable name="xor" units="dimensionless" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>and</ci>
        <piecewise>
          <piece>
            <cn cellml:units="dimensionless">1</cn>
            <apply>
              <and />
              <apply>
                <eq />
                <ci>x</ci>
                <cn cellml:units="dimensionless">0</cn>
              </apply>
              <apply>
                <eq />
                <ci>y</ci>
                <cn cellml:units="dimensionless">1</cn>
              </apply>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="dimensionless">0</cn>
          </otherwise>
        </piecewise>
      </apply>
      <apply>
        <eq/>
        <ci>or</ci>
        <piecewise>
          <piece>
            <cn cellml:units="dimensionless">1</cn>
            <apply>
              <or />
              <apply>
                <eq />
                <ci>x</ci>
                <cn cellml:units="dimensionless">0</cn>
              </apply>
              <apply>
                <eq />
                <ci>y</ci>
                <cn cellml:units="dimensionless">1</cn>
              </apply>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="dimensionless">0</cn>
          </otherwise>
        </piecewise>
      </apply>
      <apply>
        <eq/>
        <ci>xor</ci>
        <piecewise>
          <piece>
            <cn cellml:units="dimensionless">1</cn>
            <apply>
              <xor />
              <apply>
                <eq />
                <ci>x</ci>
                <cn cellml:units="dimensionless">0</cn>
              </apply>
              <apply>
                <eq />
                <ci>y</ci>
                <cn cellml:units="dimensionless">1</cn>
              </apply>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="dimensionless">0</cn>
          </otherwise>
        </piecewise>
      </apply>
      <apply>
        <eq/>
        <ci>xor</ci>
        <piecewise>
          <piece>
            <cn cellml:units="dimensionless">1</cn>
            <apply>
              <xor />
              <apply>
                <eq />
                <ci>x</ci>
                <cn cellml:units="dimensionless">0</cn>
              </apply>
              <apply>
                <eq />
                <ci>y</ci>
                <cn cellml:units="dimensionless">1</cn>
              </apply>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="dimensionless">0</cn>
          </otherwise>
        </piecewise>
      </apply>
    </math>
  </component>
</model>
