<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.3 Extension elements can go in a component_ref -->
<model name="component_ref_with_extensions"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:fruit="http://fruit.org">
  <component name="A" />
  <component name="B" />
  <group>
    <relationship_ref relationship="containment" />
    <component_ref component="A">
      <component_ref component="B" fruit:banana="True">
        <fruit:orange peel="yes">
          <fruit:clementine />
        </fruit:orange>
        <fruit:pear>Tinned</fruit:pear>
      </component_ref>
    </component_ref>
  </group>
</model>
