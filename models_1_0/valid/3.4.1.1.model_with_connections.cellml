<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.0, 3.4.1.1: A model may contain connection elements -->
<model name="model_with_connections"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="dimensionless" initial_value="0" public_interface="out" />
  </component>
  <component name="B">
    <variable name="a" units="dimensionless" public_interface="in" />
  </component>
  <component name="C">
    <variable name="a" units="dimensionless" public_interface="in" />
  </component>
  <connection>
    <map_components component_1="A" component_2="B" />
    <map_variables variable_1="a" variable_2="a" />
  </connection>
  <connection>
    <map_components component_1="A" component_2="C" />
    <map_variables variable_1="a" variable_2="a" />
  </connection>
</model>
