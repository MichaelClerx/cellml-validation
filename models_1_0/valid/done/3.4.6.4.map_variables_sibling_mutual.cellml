<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 3.4.6.4: Siblings are connected via the public interface -->
<!-- A.a to B.a, and B.b to A.b (interdependent components) -->
<model name="map_variables_sibling_mutual"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="a" units="dimensionless" public_interface="out" initial_value="1" />
    <variable name="b" units="dimensionless" public_interface="in" />
  </component>
  <component name="B">
    <variable name="a" units="dimensionless" public_interface="in" />
    <variable name="b" units="dimensionless" public_interface="out" initial_value="2" />
  </component>
  <connection>
    <map_components component_1="A" component_2="B" />
    <map_variables variable_1="a" variable_2="a" />
    <map_variables variable_1="b" variable_2="b" />
  </connection>
</model>
