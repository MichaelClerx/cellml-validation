<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 5.4.2.7: A unit with a non-zero offset must have exponent 1 -->
<model name="unit_offset_non_zero_and_exponent_one"
       xmlns="http://www.cellml.org/cellml/1.0#">
  <units name="wooster">
    <unit units="volt" offset="2" exponent="1" />
  </units>
  <units name="fluther">
    <unit units="volt" offset="2" /> <!-- default exponent is 1 -->
  </units>
</model>
