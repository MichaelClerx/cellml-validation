<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.3 Extension attributes & elements can go in a map_components -->
<model name="connection_with_extensions"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:fruit="http://fruit.org">
  <connection>
    <map_components component_1="A" component_2="B" fruit:x_a_day="5">
      <fruit:orange peel="yes">
        <fruit:clementine />
      </fruit:orange>
      <fruit:pear>Tinned</fruit:pear>
    </map_components>
    <map_variables variable_1="a" variable_2="a" />
  </connection>
  <component name="A">
    <variable name="a" units="dimensionless" public_interface="out" />
  </component>
  <component name="B">
    <variable name="a" units="dimensionless" public_interface="in" />
  </component>
</model>
