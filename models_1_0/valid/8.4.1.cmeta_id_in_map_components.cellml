<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 8.4.1: A map_components can have a cmeta:id -->
<model name="cmeta_id_in_map_components"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
  <connection>
    <map_components component_1="a" component_2="b" cmeta:id="x" />
    <map_variables variable_1="x" variable_2="x" />
  </connection>
  <component name="a">
    <variable name="x" units="newton" public_interface="out" />
  </component>
  <component name="b">
    <variable name="x" units="newton" public_interface="in" />
  </component>
</model>
