<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.3 Extension attributes & elements can go in a variable_ref -->
<model name="variable_ref_with_extensions"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:fruit="http://fruit.org">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref variable="a" fruit:x_a_day="5">
        <fruit:orange peel="yes">
          <fruit:clementine />
        </fruit:orange>
        <fruit:pear>Tinned</fruit:pear>
        <role role="reactant" />
      </variable_ref>
    </reaction>
  </component>
</model>
