<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 2.4.3 Extension attributes & elements can go in a variable -->
<model name="variable_with_extensions"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:fruit="http://fruit.org">
  <component name="A">
    <variable name="a" units="dimensionless" fruit:x_a_day="5">
      <fruit:orange peel="yes">
        <fruit:clementine />
      </fruit:orange>
      <fruit:pear>Tinned</fruit:pear>
    </variable>
  </component>
</model>
