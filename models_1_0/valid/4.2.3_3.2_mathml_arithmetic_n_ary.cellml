<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 4.2.3: The MathML "CellML subset"
N-ary plus, minus and times -->
<model name="mathml_arithmetic_n_ary"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="plus" units="volt" />
    <variable name="minus" units="volt" />
    <variable name="times" units="volt" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>plus</ci>
        <apply>
          <plus />
          <cn cellml:units="volt">1</cn>
          <cn cellml:units="volt">2</cn>
          <cn cellml:units="volt">3</cn>
          <cn cellml:units="volt">4</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>minus</ci>
        <apply>
          <minus />
          <cn cellml:units="volt">1.2</cn>
          <cn cellml:units="volt">3.4</cn>
          <cn cellml:units="volt">5.6</cn>
          <cn cellml:units="volt">7.8</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>times</ci>
        <apply>
          <times />
          <cn cellml:units="dimensionless">2</cn>
          <cn cellml:units="dimensionless">3</cn>
          <cn cellml:units="dimensionless">4</cn>
          <cn cellml:units="volt">1.2</cn>
        </apply>
      </apply>
    </math>
  </component>
</model>
