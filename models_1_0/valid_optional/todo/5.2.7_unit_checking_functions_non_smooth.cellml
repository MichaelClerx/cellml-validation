<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML 1.0, 5.2.7: MathML unit checking, floor, ceiling, abs -->
<model name="unit_checking_mathml_functions_non_smooth"
       xmlns="http://www.cellml.org/cellml/1.0#"
       xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="A">
    <variable name="floor" units="newton" />
    <variable name="ceiling" units="newton" />
    <variable name="abs" units="newton" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>floor</ci>
        <cn cellml:units="newton">1.2</cn>
      </apply>
      <apply>
        <eq/>
        <ci>ceiling</ci>
        <apply>
          <ceiling />
          <cn cellml:units="newton">3</cn>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>abs</ci>
        <apply>
          <abs />
          <cn cellml:units="newton">3</cn>
        </apply>
      </apply>
    </math>
  </component>
</model>
