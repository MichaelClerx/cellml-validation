              </apply>
            </apply>
            <apply>
              <times/>
              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.0365 </cn>
              <ci> sigma </ci>
              <apply>
                <exp/>
                <apply>
                  <minus/>
                  <apply>
                    <divide/>
                    <apply>
                      <times/>
                      <ci> V </ci>
                      <ci> F </ci>
