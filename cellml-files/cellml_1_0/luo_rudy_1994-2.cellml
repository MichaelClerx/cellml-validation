        <ci> beta_K1 </ci>
        <apply>
          <divide/>
          <apply>
            <plus/>
            <apply>
              <times/>
              <cn cellml:units="per_ms"> 0.49124 </cn>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <cn cellml:units="dimensionless"> 0.08032 </cn>
                  <apply>
                    <minus/>
