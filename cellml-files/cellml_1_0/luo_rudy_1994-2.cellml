        <ci> E_K </ci>
        <apply>
          <times/>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> R </ci>
              <ci> T </ci>
            </apply>
            <ci> F </ci>
          </apply>
          <apply>
            <ln/>
            <apply>
              <divide/>
              <apply>
                <plus/>
                <ci> Ko </ci>
                <apply>
                  <times/>
