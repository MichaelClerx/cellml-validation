                <times/>
                <cn cellml:units="dimensionless"> 0.06175 </cn>
                <apply>
                  <minus/>
                  <ci> V </ci>
                  <apply>
                    <plus/>
                    <ci> E_K1 </ci>
                    <cn cellml:units="mV"> 594.31 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
                <times/>
                <cn cellml:units="per_mV"> -0.5143 </cn>
                <apply>
                  <plus/>
