<!-- CellML 1.1, 2.4.4 A group cannot have text -->
<model name="text_in_group"
       xmlns="http://www.cellml.org/cellml/1.1#">
  <component name="A" />
  <component name="B" />
  <group>
    <relationship_ref relationship="containment" />
    <component_ref component="A">
      <component_ref component="B" />
    </component_ref>
    Bananas
  </group>
</model>
