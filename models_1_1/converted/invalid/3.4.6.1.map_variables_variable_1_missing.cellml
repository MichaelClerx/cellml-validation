<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.1, 3.4.6.1: Map_variables must have an attribute variable_1 -->
<model name="map_variables_variable_1_missing"
       xmlns="http://www.cellml.org/cellml/1.1#">
  <component name="A">
    <variable name="a" units="volt" public_interface="out" />
  </component>
  <component name="B">
    <variable name="a" units="volt" public_interface="in" />
  </component>
  <connection>
    <map_components component_1="A" component_2="B" />
    <map_variables variable_2="a" />
  </connection>
</model>
