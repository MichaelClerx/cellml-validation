<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.1, 4.2.3: The MathML "CellML subset.
Binary plus, minus, times, divide -->
<model name="mathml_arithmetic_binary"
       xmlns="http://www.cellml.org/cellml/1.1#"
       xmlns:cellml="http://www.cellml.org/cellml/1.1#">
  <component name="A">
    <variable name="a" units="volt" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>a</ci>
        <apply>
          <plus />
          <apply>
            <times />
            <apply>
              <minus />
              <cn cellml:units="volt">3</cn>
              <cn cellml:units="volt">1.2</cn>
            </apply>
            <apply>
              <divide />
              <cn cellml:units="kilogram">-2</cn>
              <cn cellml:units="kilogram">8.7</cn>
            </apply>
          </apply>
          <cn cellml:units="volt">-1</cn>
        </apply>
      </apply>
    </math>
  </component>
</model>
