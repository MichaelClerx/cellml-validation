<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.1, 7.4.2.1: A variable_ref must declare a variable attribute -->
<model name="variable_ref_variable_missing"
       xmlns="http://www.cellml.org/cellml/1.1#">
  <component name="A">
    <variable name="a" units="mole" />
    <reaction>
      <variable_ref>
        <role role="reactant" />
      </variable_ref>
    </reaction>
  </component>
</model>
