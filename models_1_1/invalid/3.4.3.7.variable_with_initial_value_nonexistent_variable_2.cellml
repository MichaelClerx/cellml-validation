<?xml version="1.0" encoding="UTF-8"?>
<!-- CellML Test Suite. https://github.com/MichaelClerx/cellml-validation -->
<!-- CellML 1.1, 3.4.3.7: A variable's initial value can reference a variable,
     but only in the same component. -->
<model name="variable_with_initial_value_nonexistent_variable_2"
       xmlns="http://www.cellml.org/cellml/1.1#">
  <component name="A">
    <variable name="a" units="dimensionless" initial_value="b" />
  </component>
  <component name="B">
    <variable name="b" units="dimensionless" initial_value="1" />
  </component>
</model>
